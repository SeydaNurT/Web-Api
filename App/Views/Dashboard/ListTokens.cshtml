@model List<string>

@{
    ViewBag.Title = "Tokens";
}

<h2>Tokens</h2>

<table class="table">
    <thead>
        <tr>
            <th>Token</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var token in Model)
        {
            <tr>
                <td>@token</td>
            </tr>
        }
    </tbody>
</table>

<a class="btn btn-primary" href="@Url.Action("IndexDashboard", "Dashboard")">Back to Dashboard</a>
