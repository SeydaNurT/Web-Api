<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Surname</th>
            <th>Username</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.ID</td>
                <td>@user.FirstName</td>
                <td>@user.Surname</td>
                <td>@user.Username</td>
            </tr>
        }
    </tbody>
</table>
    